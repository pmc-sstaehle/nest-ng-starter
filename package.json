{
  "name": "nest-ng-template",
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf node_modules package-lock.json",
    "hmr": "concurrently \"nodemon \" \"ng serve --hmr --progress=false --proxy-config proxy.conf.json\" ",
    "dev": "concurrently \"nodemon \" \"ng serve --progress=false --proxy-config proxy.conf.json\"  ",
    "ng": "ng",
    "run-be-test": "nodemon",
    "run-fe-test": "ng serve --progress=false --proxy-config proxy.conf.json",
    "prestart": "npm run build",
    "start": "node dist/server/main.js",
    "build": "ng build --prod && tsc -p ./server",
    "test": "ng test",
    "test:backend": "jest --config=server/jest.json",
    "test:backend:watch": "jest --watch --config=server/jest.json",
    "test:backend:coverage": "jest --config=server/jest.json --coverage --coverageDirectory=coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:backend": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:backend:watch": "jest --watch --config=e2e/jest-e2e.json",
    "deploy-heroku": "git push heroku master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.2",
    "@ngx-gallery/core": "^4.0.3",
    "@types/dotenv": "^6.1.1",
    "@w11k/ngx-componentdestroyed": "^4.1.4",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.0.0",
    "hammerjs": "^2.0.8",
    "image-size": "^0.7.4",
    "imagesloaded": "^4.1.4",
    "masonry-layout": "^4.2.2",
    "mysql": "^2.17.1",
    "ng-masonry-grid": "^1.3.0",
    "ngx-file-drop": "^8.0.2",
    "ngx-toastr": "^10.0.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "~6.4.0",
    "sharp": "^0.22.1",
    "tslib": "^1.9.0",
    "typeorm": "^0.2.18",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
