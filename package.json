    "@angular/language-service": "~8.0.0",
    "@nestjs/testing": "^6.0.0",
    "@schematics/angular": "~8.0.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "~8.9.4",
    "@types/supertest": "^2.0.7",
    "codelyzer": "^5.0.0",
